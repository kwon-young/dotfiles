snippet input_tab "table figure with external latex"
\begin{figure}[htpb]
  \centering
  \input{${1:file}.latex}
  \caption{${2:My caption}}
  \label{tab:${3:mylabel}}
\end{figure}
endsnippet

snippet subfloat "subfloat env"
\subfloat[${1:caption}]{
  \fbox{\includegraphics[width=${2:0.45in}]{ressources/${3:image}}}}
${4:\hfill}
endsnippet

snippet only "only env"
\only<${1:1}>{$2}
endsnippet

snippet cha "chapter"
\chapter{${1}}
\label{cha:$1}
endsnippet

snippet sec "section"
\\section{${1}}
\\label{sec:${2:$1}}
${0}
endsnippet

snippet ssec "subsection"
\subsection{${1}}
\label{ssec:$1}
endsnippet

snippet sssec "subsubsection"
\\subsubsection{${1}}
\\label{sssec:$1}
endsnippet
